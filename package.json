{
  "name": "prm-cli",
  "version": "0.0.33",
  "description": "Registry Manager for Package Management Tools",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "sudo npm link",
    "build": "tsc && chmod +x ./bin/index.js",
    "type": "tsc -p ./tsconfig.json -emitDeclarationOnly",
    "pub": "npm publish --registry https://registry.npmjs.org"
  },
  "bin": {
    "prm": "./bin/index.js"
  },
  "keywords": [
    "registry",
    "npm",
    "package management tools"
  ],
  "author": "荣顶",
  "license": "ISC",
  "homepage": "https://github.com/wangrongding/prm-cli/",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangrongding/prm-cli.git"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "commander": "^9.5.0",
    "npm": "3.10.3",
    "request": "^2.88.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/node": "^18.19.3"
  }
}
